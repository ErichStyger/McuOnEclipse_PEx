%-BW_METHOD_BEGIN UpdateFull
%ifdef UpdateFull
%include Common\SSD1306UpdateFull.Inc
void %'ModuleName'%.%UpdateFull(void)
{
#if %'ModuleName'_CONFIG_SSD1306_DRIVER_TYPE==1306 /* SSD1306 */
  (void)SSD1306_SetPageStartAddr(0);
  (void)SSD1306_SetColStartAddr(0);
  SSD1306_WriteDataBlock(&%'ModuleName'%.DisplayBuf[0][0], sizeof(%'ModuleName'%.DisplayBuf));
#elif %'ModuleName'_CONFIG_SSD1306_DRIVER_TYPE==1106 /* SH1106 */
  /* the SSH1306 has a 132x64 memory organization (compared to the 128x64 of the SSD1306) */
  int page;

  for(page=0; page<%'ModuleName'%.DISPLAY_HW_NOF_PAGES; page++) {
    (void)SSD1306_SetPageStartAddr(page);
    (void)SSD1306_SetColStartAddr(0);
    SSD1306_WriteDataBlock(&%'ModuleName'%.DisplayBuf[0][0]+(page*%'ModuleName'%.DISPLAY_HW_NOF_COLUMNS), %'ModuleName'%.DISPLAY_HW_NOF_COLUMNS);
  }
#else
  #error "unknown display type?"
#endif
}

%endif %- UpdateFull
%-BW_METHOD_END UpdateFull
