uint8_t %'ModuleName'%.%DisplayInvert(bool invert)
{
  if(invert) {
    SSD1306_WriteCommand(SSD1306_INVERT_DISPLAY);
  } else {
    SSD1306_WriteCommand(SSD1306_NORMAL_DISPLAY);
  }
  return ERR_OK;
}

%endif %- DisplayInvert
%-BW_METHOD_END DisplayInvert
%-BW_METHOD_BEGIN SetRowCol
%ifdef SetRowCol
%define! Parcol
%define! Parrow
%define! RetVal
%include Common\SSD1306SetRowCol.Inc
uint8_t %'ModuleName'%.%SetRowCol(uint8_t row, uint8_t col)
{
  uint8_t res;

  res = SSD1306_SetPageStartAddr(row);
  if (res!=ERR_OK) {
    return res;
  }
  return SSD1306_SetColStartAddr(col);
}

%endif %- SetRowCol
%-BW_METHOD_END SetRowCol
%-************************************************************************************************************
%-BW_METHOD_BEGIN PrintString
%ifdef PrintString
%define! Parstr
%define! Parline
%define! Parcol
%include Common\SSD1306PrintString.Inc
void %'ModuleName'%.%PrintString(uint8_t line, uint8_t col, uint8_t *str)
{
  if (%'ModuleName'%.%SetRowCol(line, col)!=ERR_OK) {
    return; /* error! */
  }
  while(*str != '\0'){
    if(*str == '\n') {
      actPage++;
      if(actPage >= %'ModuleName'%.DISPLAY_HW_NOF_PAGES) {
        actPage=0;
      }
      SSD1306_SetPageStartAddr(actPage);
      SSD1306_SetColStartAddr(0);
      str++;
    } else {
      SSD1306_PrintChar(*str);
      str++;
    }
  }
}

%endif %- PrintString
%-BW_METHOD_END PrintString
%-************************************************************************************************************
%-BW_METHOD_BEGIN ClearLine
%ifdef ClearLine
%define! Parline
%include Common\SSD1306ClearLine.Inc
void %'ModuleName'%.%ClearLine(uint8_t line)
{
  uint8_t i;

  if (%'ModuleName'%.%SetRowCol(line, 0)!=ERR_OK) {
    return; /* error! */
  }
  for(i=0; i<%'ModuleName'%.DISPLAY_HW_NOF_COLUMNS; i++) {
    SSD1306_WriteData(0); /* clear column */
  }
}

%endif %- ClearLine
%-BW_METHOD_END ClearLine
