%-BW_METHOD_BEGIN Clear
%ifdef Clear
%include Common\SSD1306Clear.Inc
void %'ModuleName'%.%Clear(void)
{
  int i;
  uint8_t *p = &%'ModuleName'%.DisplayBuf[0][0];

  for(i=0; i<sizeof(%'ModuleName'%.DisplayBuf); i++) {
    *p = 0x00;
    p++;
  }
  %'ModuleName'%.%UpdateFull();
}

%endif %- Clear
%-BW_METHOD_END Clear
