%-BW_METHOD_BEGIN SetDisplayOrientation
%ifdef SetDisplayOrientation
%define! ParnewOrientation
%include Common\SSD1306SetDisplayOrientation.Inc
void %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'_DisplayOrientation newOrientation)
{
  currentOrientation = newOrientation;
  switch(newOrientation) {
    case %'ModuleName'%.ORIENTATION_LANDSCAPE:
      SSD1306_WriteCommand(SSD1306_MEMORY_MODE);                 %>70/* set memory mode */
      SSD1306_WriteCommand(0x00);                                %>70/* 00b: horizontal addressing mode mode */
      SSD1306_WriteCommand(SSD1306_SEG_REMAP | 0x01);            %>70/* set segment re-map 128 to 0 */
      SSD1306_WriteCommand(SSD1306_COM_SCAN_DEC);                %>70/* set COM output scan direction 64 to 0 */
      break;
    case %'ModuleName'%.ORIENTATION_LANDSCAPE180:
      SSD1306_WriteCommand(SSD1306_MEMORY_MODE);                 %>70/* set memory mode */
      SSD1306_WriteCommand(0x00);                                %>70/* 00b: horizontal addressing mode mode */
      SSD1306_WriteCommand(SSD1306_SEG_REMAP);                   %>70/* set segment re-map 0 to 128 */
      SSD1306_WriteCommand(SSD1306_COM_SCAN_INC);                %>70/* set COM output scan direction 0 to 64 */
      break;
    case %'ModuleName'%.ORIENTATION_PORTRAIT:
      SSD1306_WriteCommand(SSD1306_MEMORY_MODE);                 %>70/* set memory mode */
      SSD1306_WriteCommand(0x01);                                %>70/* 01b: vertical addressing mode */
      SSD1306_WriteCommand(SSD1306_SEG_REMAP | 0x01);            %>70/* set segment re-map 128 to 0 */
      SSD1306_WriteCommand(SSD1306_COM_SCAN_INC);                %>70/* set COM output scan direction 0 to 64 */
      break;
    case %'ModuleName'%.ORIENTATION_PORTRAIT180:
      SSD1306_WriteCommand(SSD1306_MEMORY_MODE);                 %>70/* set memory mode */
      SSD1306_WriteCommand(0x01);                                %>70/* 01b: vertical addressing mode */
      SSD1306_WriteCommand(SSD1306_SEG_REMAP);                   %>70/* set segment re-map 0 to 128 */
      SSD1306_WriteCommand(SSD1306_COM_SCAN_DEC);                %>70/* set COM output scan direction 64 to 0 */
      break;
    default:
      break;
  } /* switch */
}

%endif %- SetDisplayOrientation
%-BW_METHOD_END SetDisplayOrientation
