%-BW_METHOD_BEGIN Init
%ifdef Init
%include Common\SSD1306Init.Inc
void %'ModuleName'%.%Init(void)
{
  /* see as well https://github.com/hallard/ArduiPi_OLED/blob/master/ArduiPi_OLED.cpp */
#if %'ModuleName'_CONFIG_INIT_DELAY_MS>0
  %@Wait@'ModuleName'%.Waitms(%'ModuleName'_CONFIG_INIT_DELAY_MS);%>70/* give hardware time to power up*/
#endif
#if %'ModuleName'_CONFIG_SSD1306_HAS_RST
  %'ModuleName'_CONFIG_SSD1306_RESET_LOW();
  %@Wait@'ModuleName'%.Waitms(1);                                %>70/* wait for 1 ms */
  %'ModuleName'_CONFIG_SSD1306_RESET_HIGH();
#endif
  SSD1306_WriteCommand(SSD1306_DISPLAY_OFF);                     %>70/* turn off display */
  SSD1306_WriteCommand(SSD1306_SET_DISPLAY_CLOCK_DIV);           %>70/* set display clock divide ratio/oscillator frequency */
  SSD1306_WriteCommand(0x80);                                    %>70/* the suggested ratio 0x80 */

  SSD1306_WriteCommand(SSD1306_SET_MULTIPLEX);                   %>70/* set multiplex ratio(1 to 64) */
  SSD1306_WriteCommand(%'ModuleName'%.DISPLAY_HW_NOF_ROWS-1);    %>70/* multiplex depending on number of rows */

  SSD1306_WriteCommand(SSD1306_SET_DISPLAY_OFFSET);              %>70/* set display offset */
  SSD1306_WriteCommand(0x00);                                    %>70/* no offset */
  SSD1306_WriteCommand(SSD1306_SET_START_LINE | 0x00);           %>70/* set start line address */

  SSD1306_WriteCommand(SSD1306_COLUMN_ADDR);                     %>70/* set start and end address of the columns */
  SSD1306_WriteCommand(0);
  SSD1306_WriteCommand(%'ModuleName'%.DISPLAY_HW_NOF_COLUMNS-1);

  SSD1306_WriteCommand(SSD1306_PAGE_ADDR);                       %>70/* set start and end address of the pages */
  SSD1306_WriteCommand(0);
  SSD1306_WriteCommand(%'ModuleName'%.DISPLAY_HW_NOF_PAGES-1);

  SSD1306_WriteCommand(SSD1306_CHARGE_PUMP);                     %>70/* set charge pump enable/disable */
#if %'ModuleName'_CONFIG_SSD1306_EXTERNAL == 1
  SSD1306_WriteCommand(0x10);                                    %>70/* set to disabled */
#else
  SSD1306_WriteCommand(0x14);                                    %>70/* set to enabled */
#endif
#if %'ModuleName'_CONFIG_SSD1306_SIZE_TYPE==12832
  SSD1306_WriteCommand(SSD1306_SET_COM_PINS);                    %>70/* set COM pins hardware configuration */
  SSD1306_WriteCommand(0x02);
  SSD1306_WriteCommand(SSD1306_SET_CONTRAST);                    %>70/* set contrast control register */
  SSD1306_WriteCommand(0x8F);
#elif %'ModuleName'_CONFIG_SSD1306_SIZE_TYPE==12864
  SSD1306_WriteCommand(SSD1306_SET_COM_PINS);                    %>70/* set COM pins hardware configuration */
  SSD1306_WriteCommand(0x12);
  SSD1306_WriteCommand(SSD1306_SET_CONTRAST);                    %>70/* set contrast control register */
  #if %'ModuleName'_CONFIG_SSD1306_EXTERNAL == 1
  SSD1306_WriteCommand(0x9F);
  #else
  SSD1306_WriteCommand(0xCF);
  #endif
#else
  #error "unknown display type"
#endif

  SSD1306_WriteCommand(SSD1306_SET_PRECHARGE);                   %>70/* set pre-charge period */
#if %'ModuleName'_CONFIG_SSD1306_EXTERNAL == 1
  SSD1306_WriteCommand(0x22);
#else
  SSD1306_WriteCommand(0xF1);
#endif
  SSD1306_WriteCommand(SSD1306_SET_VCOM_DETECT);                 %>70/* set vcomh */
  SSD1306_WriteCommand(0x40);

  SSD1306_WriteCommand(SSD1306_DISPLAY_ALL_ON_RESUME);           %>70/* disable entire display on */
  SSD1306_WriteCommand(SSD1306_NORMAL_DISPLAY);                  %>70/* set normal display */
  SSD1306_WriteCommand(SSD1306_DEACTIVATE_SCROLL);               %>70/* Deactivate scrolling */
  SSD1306_WriteCommand(SSD1306_DISPLAY_ON);                      %>70/* turn on oled panel */

%if defined(SetDisplayOrientation)
#if %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_PORTRAIT
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_PORTRAIT);%>40/* Portrait mode */
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_PORTRAIT180
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_PORTRAIT180);%>40/* Portrait mode, rotated 180° */
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_LANDSCAPE);%>40/* Landscape mode, rotated right 90° */
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE180
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_LANDSCAPE180);%>40/* Landscape mode, rotated left 90° */
#endif
%else
  SSD1306_WriteCommand(SSD1306_MEMORY_MODE);                     %>70/* set memory mode */
  SSD1306_WriteCommand(0x00);                                    %>70/* 00b: horizontal addressing mode mode */
  SSD1306_WriteCommand(SSD1306_SEG_REMAP | 0x01);                %>70/* set segment re-map 128 to 0 */
  SSD1306_WriteCommand(SSD1306_COM_SCAN_DEC);                    %>70/* set COM output scan direction 64 to 0 */
%endif
#if %'ModuleName'_CONFIG_CLEAR_DISPLAY_IN_INIT
  %'ModuleName'%.%Clear();
#endif
}

%endif %- Init
%-BW_METHOD_END Init
