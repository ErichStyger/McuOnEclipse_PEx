%-BW_METHOD_BEGIN SetDisplayOrientation
%ifdef SetDisplayOrientation
%define! ParnewOrientation
%include Common\SSD1331SetDisplayOrientation.Inc
void %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'_DisplayOrientation newOrientation)
{
#if %'ModuleName'_CONFIG_USE_RAM_BUFFER
  currentOrientation = newOrientation;
  return; /* if using RAM buffer, display orientation is handled by the memory buffer itself */
#elif %'ModuleName'_CONFIG_DYNAMIC_DISPLAY_ORIENTATION
  uint8_t remap;

  currentOrientation = newOrientation;
  switch(currentOrientation) {
    default:
    case %'ModuleName'%.ORIENTATION_LANDSCAPE: /* remap = 0x60 */
      remap =   /*7|6:  01 */   REMAP_COLOR_RGB565  /* 10k RGB */
                /*  5:   1 */ | REMAP_COM_SPLIT_ODD_EVEN_EN
                /*  4:   0 */ | /*1: REMAP_SCAN_DOWN_TO_UP */  REMAP_SCAN_UP_TO_DOWN    /* top of the display to bottom */
                /*  3:   0 */ | REMAP_DISABLE_LEFT_RIGHT_SWAP /* 1: REMAP_ENABLE_LEFT_RIGHT_SWAP */
                /*  2:   0 */ | REMAP_ORDER_ABC /* 1: REMAP_ORDER_CBA */
                /*  1:   0 */ | /*1: REMAP_COLUMNS_RIGHT_TO_LEFT */ REMAP_COLUMNS_LEFT_TO_RIGHT
                /*  0:   0 */ | REMAP_HORIZONTAL_INCREMENT  /* 1: REMAP_VERTICAL_INCREMENT */
              ;
      break;

    case %'ModuleName'%.ORIENTATION_LANDSCAPE180: /* remap: 0x72 */
      remap =   /*7|6: 01 */   REMAP_COLOR_RGB565  /* 10k RGB */
                /*  5:  1 */ | REMAP_COM_SPLIT_ODD_EVEN_EN
                /*  4:  1 */ | REMAP_SCAN_DOWN_TO_UP /* 0: REMAP_SCAN_UP_TO_DOWN */    /* bottom of the display to top */
                /*  3:  0 */ | REMAP_DISABLE_LEFT_RIGHT_SWAP /* 1: REMAP_ENABLE_LEFT_RIGHT_SWAP */
                /*  2:  0 */ | REMAP_ORDER_ABC /* 1: REMAP_ORDER_CBA */
                /*  1:  1 */ | REMAP_COLUMNS_RIGHT_TO_LEFT /* REMAP_COLUMNS_LEFT_TO_RIGHT */
                /*  0:  0 */ | REMAP_HORIZONTAL_INCREMENT  /* 1: REMAP_VERTICAL_INCREMENT */
              ;
      break;

/* \todo */
    case %'ModuleName'%.ORIENTATION_PORTRAIT: /* remap: 0x63 */
      remap =   /*7|6:  01 */   REMAP_COLOR_RGB565  /* 10k RGB */
                /*  5:   1 */ | REMAP_COM_SPLIT_ODD_EVEN_EN
                /*  4:   0 */ | /*1: REMAP_SCAN_DOWN_TO_UP */  REMAP_SCAN_UP_TO_DOWN    /* top of the display to bottom */
                /*  3:   0 */ | REMAP_DISABLE_LEFT_RIGHT_SWAP /* 1: REMAP_ENABLE_LEFT_RIGHT_SWAP */
                /*  2:   0 */ | REMAP_ORDER_ABC /* 1: REMAP_ORDER_CBA */
                /*  1:   1 */ | REMAP_COLUMNS_RIGHT_TO_LEFT /* 0: REMAP_COLUMNS_LEFT_TO_RIGHT */
                /*  0:   1 */ | /* 0: REMAP_HORIZONTAL_INCREMENT */ REMAP_VERTICAL_INCREMENT
              ;
      break;

/* \todo */
    case %'ModuleName'%.ORIENTATION_PORTRAIT180:
      remap =   /*7|6:  01 */   REMAP_COLOR_RGB565  /* 10k RGB */
                /*  5:   1 */ | REMAP_COM_SPLIT_ODD_EVEN_EN
                /*  4:   0 */ | /*1: REMAP_SCAN_DOWN_TO_UP */  REMAP_SCAN_UP_TO_DOWN    /* top of the display to bottom */
                /*  3:   0 */ | REMAP_DISABLE_LEFT_RIGHT_SWAP /* 1: REMAP_ENABLE_LEFT_RIGHT_SWAP */
                /*  2:   0 */ | REMAP_ORDER_ABC /* 1: REMAP_ORDER_CBA */
                /*  1:   1 */ | REMAP_COLUMNS_RIGHT_TO_LEFT /* 0: REMAP_COLUMNS_LEFT_TO_RIGHT */
                /*  0:   1 */ | /* 0: REMAP_HORIZONTAL_INCREMENT */ REMAP_VERTICAL_INCREMENT
              ;
      break;
  } /* switch */
  %'ModuleName'%.WriteCommand(%'ModuleName'%.CMD_SETREMAP); /* Remap command */
  %'ModuleName'%.WriteCommand(remap);                 /* remap data */
#endif
}

