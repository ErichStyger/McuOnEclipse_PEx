%-BW_METHOD_BEGIN InitCommChannel
%ifdef InitCommChannel
%include Common\SSD1331InitCommChannel.Inc
void %'ModuleName'%.%InitCommChannel(void)
{
  /* settings:
    - Max 16 MHz
    - Send MSB first
    - Shift clock idle polarity: low
    - Clock edge: falling edge (CPOL=0,CPHA=0)
  */
%if defined(HWSPI)
  %@HWSPI@'ModuleName'%.SetIdleClockPolarity(0); /* low idle clock polarity */
  %@HWSPI@'ModuleName'%.SetShiftClockPolarity(0); /* falling edge shift clock */
  %@HWSPI@'ModuleName'%.SetBaudRateMode(%HWSPIBaudRateMode); /* select baud rate */
%endif
}

%endif %- InitCommChannel
%-BW_METHOD_END InitCommChannel
%-************************************************************************************************************
%-BW_METHOD_BEGIN Init
%ifdef Init
%include Common\SSD1331Init.Inc
void %'ModuleName'%.%Init(void)
{
  /* 3-Wire SPI:
     CLK: Data needs to be valid at rising clock edge.
     MOSI: shift MSB first
  */
  /* reset: You might need to do an extra delay (up to 2 secs) before calling this function! */
  /* %@Wait@'ModuleName'%.Waitms(2000); */
  RESET_LOW();
  %@Wait@'ModuleName'%.Waitms(200);
  RESET_HIGH();

  /* Initialization Sequence */
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_DISPLAYOFF);   // 0xAE
#if 0
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_SETREMAP);   // 0xA0
#if %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE || %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE180s
  #if defined SSD1331_COLORORDER_RGB
  %'ModuleName'%.%WriteCommand(0x72);       // RGB Color
  #else
  %'ModuleName'%.%WriteCommand(0x76);       // BGR Color
  #endif
#else /* set A[0] to 1 ver vertical address increment */
  #if defined SSD1331_COLORORDER_RGB
  %'ModuleName'%.%WriteCommand(0x72|0x01);       // RGB Color
  #else
  %'ModuleName'%.%WriteCommand(0x76|0x01);       // BGR Color
  #endif
#endif
#endif
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_STARTLINE);  // 0xA1
  %'ModuleName'%.%WriteCommand(0x0);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_DISPLAYOFFSET);  // 0xA2
  %'ModuleName'%.%WriteCommand(0x0);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_NORMALDISPLAY);    // 0xA4
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_SETMULTIPLEX);   // 0xA8
  %'ModuleName'%.%WriteCommand(0x3F);       // 0x3F 1/64 duty
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_SETMASTER);    // 0xAD
  %'ModuleName'%.%WriteCommand(0x8E);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_POWERMODE);    // 0xB0
  %'ModuleName'%.%WriteCommand(0x0B);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_PRECHARGE);    // 0xB1
  %'ModuleName'%.%WriteCommand(0x31);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_CLOCKDIV);   // 0xB3
  %'ModuleName'%.%WriteCommand(0xF0);  // 7:4 = Oscillator Frequency, 3:0 = CLK Div Ratio (A[3:0]+1 = 1..16)
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_PRECHARGEA);   // 0x8A
  %'ModuleName'%.%WriteCommand(0x64);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_PRECHARGEB);   // 0x8B
  %'ModuleName'%.%WriteCommand(0x78);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_PRECHARGEA);   // 0x8C
  %'ModuleName'%.%WriteCommand(0x64);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_PRECHARGELEVEL);   // 0xBB
  %'ModuleName'%.%WriteCommand(0x3A);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_VCOMH);      // 0xBE
  %'ModuleName'%.%WriteCommand(0x3E);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_MASTERCURRENT);    // 0x87
  %'ModuleName'%.%WriteCommand(0x06);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_CONTRASTA);    // 0x81
  %'ModuleName'%.%WriteCommand(0x91);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_CONTRASTB);    // 0x82
  %'ModuleName'%.%WriteCommand(0x50);
  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_CONTRASTC);    // 0x83
  %'ModuleName'%.%WriteCommand(0x7D);

#if %'ModuleName'_CONFIG_USE_RAM_BUFFER
  {
  uint8_t remap;

  remap =   /*7|6:  01 */   REMAP_COLOR_RGB565  /* 10k RGB */
            /*  5:   1 */ | REMAP_COM_SPLIT_ODD_EVEN_EN
            /*  4:   0 */ | /*1: REMAP_SCAN_DOWN_TO_UP */  REMAP_SCAN_UP_TO_DOWN    /* top of the display to bottom */
            /*  3:   0 */ | REMAP_DISABLE_LEFT_RIGHT_SWAP /* 1: REMAP_ENABLE_LEFT_RIGHT_SWAP */
            /*  2:   0 */ | REMAP_ORDER_ABC /* 1: REMAP_ORDER_CBA */
            /*  1:   0 */ | /*1: REMAP_COLUMNS_RIGHT_TO_LEFT */ REMAP_COLUMNS_LEFT_TO_RIGHT
            /*  0:   0 */ | REMAP_HORIZONTAL_INCREMENT  /* 1: REMAP_VERTICAL_INCREMENT */
              ;
  %'ModuleName'%.WriteCommand(%'ModuleName'%.CMD_SETREMAP); /* Remap command */
  %'ModuleName'%.WriteCommand(remap);                 /* remap data */
  #if %'ModuleName'%.CONFIG_DYNAMIC_DISPLAY_ORIENTATION
  currentOrientation = %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION;
  #endif
  }
#elif %'ModuleName'_CONFIG_DYNAMIC_DISPLAY_ORIENTATION
#if %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_PORTRAIT
%if defined(SetDisplayOrientation)
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_PORTRAIT);%>40/* Portrait mode */
%else
  McuLib_SetDisplayOrientation(%'ModuleName'%.ORIENTATION_PORTRAIT);%>40/* Portrait mode */
%endif
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_PORTRAIT180
%if defined(SetDisplayOrientation)
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_PORTRAIT180);%>40/* Portrait mode, rotated 180° */
%else
  McuLib_SetDisplayOrientation(%'ModuleName'%.ORIENTATION_PORTRAIT180);%>40/* Portrait mode, rotated 180° */
%endif
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE
%if defined(SetDisplayOrientation)
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_LANDSCAPE);%>40/* Landscape mode, rotated right 90° */
%else
  McuLib_SetDisplayOrientation(%'ModuleName'%.ORIENTATION_LANDSCAPE);%>40/* Landscape mode, rotated right 90° */
%endif
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE180
%if defined(SetDisplayOrientation)
  %'ModuleName'%.%SetDisplayOrientation(%'ModuleName'%.ORIENTATION_LANDSCAPE180);%>40/* Landscape mode, rotated left 90° */
%else
  McuLib_SetDisplayOrientation(%'ModuleName'%.ORIENTATION_LANDSCAPE180);%>40/* Landscape mode, rotated left 90° */
%endif
#endif
#endif /* %'ModuleName'_CONFIG_DYNAMIC_DISPLAY_ORIENTATION */

  %'ModuleName'%.%WriteCommand(%'ModuleName'%.CMD_DISPLAYON);  //--turn on oled panel

#if %'ModuleName'_CONFIG_CLEAR_DISPLAY_IN_INIT
  %'ModuleName'%.%Clear();
#endif
}

%endif %- Init
%-BW_METHOD_END Init
