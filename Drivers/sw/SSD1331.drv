%-BW_METHOD_BEGIN OpenWindow
%ifdef OpenWindow
%define! Parx0
%define! Pary0
%define! Parx1
%define! Pary1
%include Common\SSD1331OpenWindow.Inc
void %'ModuleName'%.%OpenWindow(%'ModuleName'_PixelDim x0, %'ModuleName'_PixelDim y0, %'ModuleName'_PixelDim x1, %'ModuleName'_PixelDim y1)
{
  uint8_t c0,c1; /* column start and end */
  uint8_t r0,r1; /* row start and end */

  if ((x0 >= %'ModuleName'%.GetWidth()) || (x1 >= %'ModuleName'%.GetWidth())) {
    return;
  }
  if ((y0 >= %'ModuleName'%.GetHeight()) || (y1 >= %'ModuleName'%.GetHeight())) {
    return;
  }
#if %'ModuleName'_CONFIG_USE_RAM_BUFFER
  /* orientation is handled by the writes to the RAM buffer, use default landscape setting */
  c0 = x0;
  c1 = x1;
  r0 = y0;
  r1 = y1;
#elif %'ModuleName'_CONFIG_DYNAMIC_DISPLAY_ORIENTATION
  switch(currentOrientation) {
    default:
    case %'ModuleName'%.ORIENTATION_LANDSCAPE:
      c0 = x0; c1 = x1;
      r0 = y0; r1 = y1;
      break;
    case %'ModuleName'%.ORIENTATION_LANDSCAPE180:
      c0 = x0; c1 = x1;
      r0 = %'ModuleName'%.HW_HEIGHT-1-y1; r1 = %'ModuleName'%.HW_HEIGHT-1-y0;
      break;
    case %'ModuleName'%.ORIENTATION_PORTRAIT:
      c0 = %'ModuleName'%.HW_WIDTH-1-y1; c1 = %'ModuleName'%.HW_WIDTH-1-y0;
      r0 = x0; r1 = x1;
      break;
    case %'ModuleName'%.ORIENTATION_PORTRAIT180:
      c0 = y0; c1 = y1;
      r0 = %'ModuleName'%.HW_HEIGHT-1-x1; r1 = %'ModuleName'%.HW_HEIGHT-1-x0;
      break;
  } /* switch */
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_PORTRAIT
  c0 = %'ModuleName'%.HW_WIDTH-1-y1; c1 = %'ModuleName'%.HW_WIDTH-1-y0;
  r0 = x0; r1 = x1;
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_PORTRAIT180
  c0 = y0; c1 = y1;
  r0 = %'ModuleName'%.HW_HEIGHT-1-x1; r1 = %'ModuleName'%.HW_HEIGHT-1-x0;
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE
  c0 = x0; c1 = x1;
  r0 = y0; r1 = y1;
#elif %'ModuleName'_CONFIG_FIXED_DISPLAY_ORIENTATION==%'ModuleName'%.CONFIG_ORIENTATION_LANDSCAPE180
  c0 = x0; c1 = x1;
  r0 = %'ModuleName'%.HW_HEIGHT-1-y1; r1 = %'ModuleName'%.HW_HEIGHT-1-y0;
#endif
  /* Set Window */
  %'ModuleName'%.WriteCommand(%'ModuleName'%.CMD_SETCOLUMN);     %>40/* set column command */
  %'ModuleName'%.WriteCommand(c0);
  %'ModuleName'%.WriteCommand(c1);

  %'ModuleName'%.WriteCommand(%'ModuleName'%.CMD_SETROW);        %>40/* set row start address command */
  //%'ModuleName'%.WriteDataWord(r1<<8|r0);                      %>40/* start and end row */
  %'ModuleName'%.WriteCommand(r0);
  %'ModuleName'%.WriteCommand(r1);
}

%endif %- OpenWindow
%-BW_METHOD_END OpenWindow
