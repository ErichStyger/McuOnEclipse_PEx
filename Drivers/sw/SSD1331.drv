  DATA_MODE();
  CS_LOW();
  %'ModuleName'%.SPI_WRITE(data);
#if %'ModuleName'_CONFIG_USE_SPI_API!=%'ModuleName'_CONFIG_SPI_API_SW
  /* for HW SPI, have to delay as transaction still might be going on! */
