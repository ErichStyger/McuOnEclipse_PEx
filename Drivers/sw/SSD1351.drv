%endif
  /*!< method to send a character */
#endif

#ifndef %'ModuleName'_CONFIG_NOF_TX_CHAR
%if defined(SWSPI)
  #define %'ModuleName'_CONFIG_NOF_TX_CHAR                       %>40 %@SWSPI@'ModuleName'%.CharsInTxBuf
%elif defined(HWSPI)
