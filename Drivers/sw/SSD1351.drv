   #define %'ModuleName'_CONFIG_SPI_API_FCT_NAME_CHECK_RX       GetCharsInRxBuf
   #define %'ModuleName'_CONFIG_SPI_API_DEVICE_HANDLE           NULL /* not used */
 %elif defined(HWSPI)
   #define %'ModuleName'_CONFIG_USE_SPI_API                     %'ModuleName'_CONFIG_SPI_API_HW
